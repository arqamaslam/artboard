.post-description a { -moz-border-radius: 100px; -ms-border-radius: 100px; -o-border-radius: 100px; -webkit-border-radius: 100px; border-radius: 100px; width: 41px; width: 41px; float: left; height: 41px; overflow: hidden; }
.replytocomment { background: #F7F7F7; float: left; width: 100%; }
.replyarea { background: #dee1e2; float: left; width: 100%; }
.replyarea input[type="file"] { background: rgba(0, 0, 0, 0) url("../img/attach.png") no-repeat scroll center center; border-right: 1px solid #c5d2d8; cursor: pointer; float: left; margin: 7px 0 0; overflow: hidden; padding: 6px 21px; text-indent: 100px; width: 70px; }
.replytocomment { float: left; overflow: hidden; padding: 0 10px 10px; width: 100%; }
.replytocomment form { -moz-border-radius: 3px; -ms-border-radius: 3px; -o-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px; overflow: hidden; width: 100%; }
.replytocomment .button { background: #29b590 none repeat scroll 0 0; border: 0 none; -moz-border-radius: 3px; -ms-border-radius: 3px; -o-border-radius: 3px; -webkit-border-radius: 3px; border-radius: 3px; color: #fff; cursor: pointer !important; font-family: proximanovas; font-size: 14px; margin: 8px; min-width: 110px; padding: 12px 16px 10px; text-align: center; text-transform: uppercase; -moz-transition: all 0.3s ease-in-out 0s; -ms-transition: all 0.3s ease-in-out 0s; -o-transition: all 0.3s ease-in-out 0s; -webkit-transition: all 0.3s ease-in-out 0s; transition: all 0.3s ease-in-out 0s; }
.replytocomment .button:hover { background: #01ac81 none repeat scroll 0 0; }
.replytocomment form textarea { background: #fff none repeat scroll 0 0; border: 1px solid #e0e0e0; -moz-border-radius: 3px 3px 0px 0px; -ms-border-radius: 3px 3px 0px 0px; -o-border-radius: 3px 3px 0px 0px; -webkit-border-radius: 3px 3px 0px 0px; border-radius: 3px 3px 0px 0px; float: left; font-size: 14px; line-height: 22px; margin-bottom: 0; height: 119px; outline: 0 none; padding: 10px 15px; width: 100% !important; font-family: 'proximanovas'; }
.image-container-main > img { width: 100%; }
.listmembmidal { padding: 30px 10px; width: 500px; }
.listmembmidal #modal1Desc { padding: 0 10px; width: 100%; }
.listmembmidal .artbmembers a:last-child { padding: 10px; width: auto; }
.listmembmidal .remodal .artbmembers p { color: #464646; float: left; font-family: proximanovar; font-size: 14px; margin: 12px 0 0; padding: 0 15px; text-align: left; width: 76%; }
/* Keyframes
   ========================================================================== */

@-webkit-keyframes remodal-opening-keyframes {
 from {
 -webkit-transform: scale(1.05);
 transform: scale(1.05);
 opacity: 0;
}
 to {
 -webkit-transform: none;
 transform: none;
 opacity: 1;
}
}
 @keyframes remodal-opening-keyframes {
 from {
 -webkit-transform: scale(1.05);
 transform: scale(1.05);
 opacity: 0;
}
 to {
 -webkit-transform: none;
 transform: none;
 opacity: 1;
}
}
 @-webkit-keyframes remodal-closing-keyframes {
 from {
 -webkit-transform: scale(1);
 transform: scale(1);
 opacity: 1;
}
 to {
 -webkit-transform: scale(0.95);
 transform: scale(0.95);
 opacity: 0;
}
}
 @keyframes remodal-closing-keyframes {
 from {
 -webkit-transform: scale(1);
 transform: scale(1);
 opacity: 1;
}
 to {
 -webkit-transform: scale(0.95);
 transform: scale(0.95);
 opacity: 0;
}
}
 @-webkit-keyframes remodal-overlay-opening-keyframes {
 from {
 opacity: 0;
}
 to {
 opacity: 1;
}
}
 @keyframes remodal-overlay-opening-keyframes {
 from {
 opacity: 0;
}
 to {
 opacity: 1;
}
}
 @-webkit-keyframes remodal-overlay-closing-keyframes {
 from {
 opacity: 1;
}
 to {
 opacity: 0;
}
}
 @keyframes remodal-overlay-closing-keyframes {
 from {
 opacity: 1;
}
 to {
 opacity: 0;
}
}

